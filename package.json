{
    "name": "polynomial",
    "title": "Polynomial.js",
    "version": "1.6.1",
    "description": "The RAW polynomial library",
    "homepage": "https://github.com/rawify/Polynomial.js",
    "bugs": "https://github.com/rawify/Polynomial.js/issues",
    "keywords": [
        "math",
        "poly",
        "polynomial",
        "number",
        "bigint",
        "complex",
        "rational",
        "parser"
    ],
    "private": false,
    "main": "./dist/polynomial.js",
    "module": "./dist/polynomial.mjs",
    "browser": "./dist/polynomial.min.js",
    "unpkg": "./dist/polynomial.min.js",
    "types": "./polynomial.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./polynomial.d.mts",
                "default": "./dist/polynomial.mjs"
            },
            "require": {
                "types": "./polynomial.d.ts",
                "default": "./dist/polynomial.js"
            },
            "browser": {
                "types": "./polynomial.d.ts",
                "default": "./dist/polynomial.min.js"
            },
            "default": {
                "types": "./polynomial.d.ts",
                "default": "./dist/polynomial.js"
            }
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "<4.7": {
            "*": [
                "polynomial.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git@github.com:rawify/Polynomial.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "example": "examples"
    },
    "scripts": {
        "build": "crude-build Polynomial",
        "test": "mocha tests/*.js"
    },
    "dependencies": {
        "complex.js": "2.4.2",
        "fraction.js": "5.2.2",
        "quaternion": "2.1.1"
    },
    "devDependencies": {
        "crude-build": "^0.1.2",
        "mocha": "*"
    }
}