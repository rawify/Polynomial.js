        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "example": "examples"
    },
    "scripts": {
        "build": "crude-build Polynomial",
        "test": "mocha tests/*.js"
    },
    "dependencies": {
        "complex.js": "*",
        "fraction.js": "*",
        "quaternion": "*"
    },
